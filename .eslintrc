{
  "extends": ["eslint:recommended", "plugin:vue/essential"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "indent": ["error", 2],
    "vue/no-unused-vars": "error",
    "quotes": ["error", "double"],
    "no-extra-semi": "off",
    "semi": ["error", "never"],
    "semi-style": ["error", "last"],
    "no-console": "off"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
