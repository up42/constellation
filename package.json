{
  "name": "@up42/constellation",
  "version": "0.0.1",
  "description": "Constellation is the UP42 Design System based on Vue Design System",
  "main": "dist/system/system.js",
  "files": [
    "dist/system"
  ],
  "scripts": {
    "dev": "webpack-dev-server --inline --config ./build/webpack.dev.conf.js",
    "start": "npm-run-all --parallel styleguide theo:onchange theo dev",
    "build": "npm-run-all theo node:build",
    "build:app": "npm run build",
    "build:system": "npm-run-all theo node:build:system",
    "node:build": "node ./build/build.js",
    "node:build:system": "node ./build/build-system.js",
    "theo": "theo ./src/tokens/tokens.json --transform web --format map.scss,scss,raw.json,json --dest ./src/assets/tokens",
    "theo:onchange": "onchange \"./src/tokens/*.json\" -- npm run theo",
    "test": "npm-run-all theo unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/up42/constellation.git"
  },
  "keywords": [
    "up42",
    "design",
    "system",
    "constellation",
    "tokens",
    "design",
    "tokens",
    "ui",
    "css",
    "scss"
  ],
  "author": "Matteo Pescarin <matteo.pescarin@up42.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/up42/constellation/issues"
  },
  "homepage": "https://github.com/up42/constellation#readme",
  "dependencies": {
    "autoprefixer": "^9.6.5",
    "babel-loader": "^8.0.6",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "html-loader": "^0.5.5",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^4.0.2",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "postcss-url": "^8.0.0",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.3.1",
    "sass-resources-loader": "^2.0.1",
    "shelljs": "^0.8.3",
    "theo": "^8.1.4",
    "url-loader": "^2.2.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webfontloader": "^1.6.28",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-merge": "^4.2.2",
    "webpack-merge-and-include-globally": "^2.1.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "css-loader": "^3.2.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "node-notifier": "^6.0.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.0",
    "portfinder": "^1.0.25",
    "semver": "^6.3.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "engines": {
    "node": ">= 8.6.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not Explorer > 0",
    "IE 11",
    "not ExplorerMobile > 0",
    "not BlackBerry > 0",
    "not OperaMini all",
    "not OperaMobile > 0"
  ]
}
